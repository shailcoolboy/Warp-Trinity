# Clock board configurator
BEGIN IO_INTERFACE
	ATTRIBUTE IOTYPE = WARP_CLKBRD_CONFIG_V1
	ATTRIBUTE INSTANCE = clk_board_config

	PORT sys_clk = CLK_100MHZ_OSC
